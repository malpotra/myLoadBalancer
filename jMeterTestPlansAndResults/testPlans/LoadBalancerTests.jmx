          <JSR223Assertion guiclass="TestBeanGUI" testclass="JSR223Assertion" testname="Server Identification Assertion">
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="script">String jsonString = vars.get(&quot;serverName&quot;);
log.info (&quot;The example answer is &quot; + jsonString);
if (	jsonString == &quot;Server 1&quot;){
	AssertionResult.setFailureMessage(&quot;Server 1 responded&quot;);
    	AssertionResult.setFailure(true); 
}
</stringProp>
            <stringProp name="scriptLanguage">groovy</stringProp>
            <stringProp name="TestPlan.comments">If response is from Server 1 then we say the test failed orherwise the test passed</stringProp>
          </JSR223Assertion>
